{
  "name": "cwrc-public-entity-dialogs",
  "version": "1.16.4",
  "description": "Dialogs for the CWRC-Writer that lookup people, places, organizations, and publications in public authority files.",
  "keywords": [
    "xml",
    "rdf",
    "editing",
    "linked",
    "data"
  ],
  "homepage": "https://github.com/cwrc/CWRC-PublicEntityDialogs#readme",
  "bugs": {
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs/issues"
  },
  "license": "GPL-2.0",
  "contributors": [
    "James Chartrand <jc.chartrand@gmail.com>",
    "Andrew MacDonald <andrewjames.code@gmail.com>",
    "Luciano Frizzera <lucaju@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs.git"
  },
  "main": "src/index.js",
  "scripts": {
    "cm": "git-cz",
    "test": "jest",
    "report-coverage": "cat ./coverage/coverage-final.json | codecov",
    "docs": "jsdoc2md src/index.js > API.md",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "broadcast-channel": "^3.0.4",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "codecov": "^3.6.5",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "dbpedia-entity-lookup": "^0.6.1",
    "geonames-entity-lookup": "^1.1.7",
    "getty-entity-lookup": "^1.4.5",
    "git-cz": "^4.3.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jsdoc-to-markdown": "^5.0.0",
    "semantic-release": "^17.0.3",
    "travis-deploy-once": "^5.0.11",
    "viaf-entity-lookup": "^1.2.5",
    "wikidata-entity-lookup": "^1.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/index.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 60,
        "statements": 60
      }
    },
    "testMatch": [
      "**/test/test.js"
    ]
  }
}
