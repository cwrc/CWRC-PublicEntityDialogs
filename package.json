{
  "name": "cwrc-public-entity-dialogs",
  "version": "0.0.0-development",
  "description": "Dialogs for the CWRC-Writer that lookup people, places, organizations, and publications in public authority files.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs.git"
  },
  "keywords": [
    "xml",
    "rdf",
    "editing",
    "linked",
    "data"
  ],
  "contributors": ["James Chartrand", "Andrew MacDonald"],
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs/issues"
  },
  "homepage": "https://github.com/cwrc/CWRC-PublicEntityDialogs#readme",
  "scripts": {
    "cm": "git-cz",
    "test": "jest",
    "report-coverage": "cat ./coverage/coverage-final.json | codecov",
    "docs": "jsdoc2md src/index.js > API.md",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "broadcast-channel": "^1.3.0",
    "jquery": "^3.4.1",
    "js-cookie": "2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "codecov": "^3.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "dbpedia-entity-lookup": "^0.4.2",
    "geonames-entity-lookup": "^1.1.7",
    "getty-entity-lookup": "^1.4.5",
    "git-cz": "^1.8.4",
    "husky": "^2.1.0",
    "jest": "^24.9.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mkdirp": "0.5.1",
    "semantic-release": "^15.13.19",
    "tape-catch": "^1.0.6",
    "travis-deploy-once": "^5.0.11",
    "viaf-entity-lookup": "^1.2.5",
    "watchify": "^3.11.1",
    "wikidata-entity-lookup": "^1.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/index.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "functions": 40,
        "lines": 50,
        "statements": 50
      }
    },
    "testMatch": [
      "**/test/test.js"
    ]
  }
}
