{
  "name": "cwrc-public-entity-dialogs",
  "version": "0.0.0-development",
  "description": "Dialogs for the CWRC-Writer that lookup people, places, organizations, and publications in public authority files.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs.git"
  },
  "keywords": [
    "xml",
    "rdf",
    "editing",
    "linked",
    "data"
  ],
  "author": "“James <“jc.chartrand@gmail.com”>",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/cwrc/CWRC-PublicEntityDialogs/issues"
  },
  "homepage": "https://github.com/cwrc/CWRC-PublicEntityDialogs#readme",
  "scripts": {
    "semantic-release": "semantic-release",
    "cm": "git-cz",
    "browserify": "mkdirp build && browserify test/development.js -o build/development.js --debug ",
    "build": "mkdirp dist && browserify src/index.js --s cwrcdialogs > dist/cwrcdialogs.js",
    "watch": "mkdirp build && watchify test/development.js -o build/development.js --debug --verbose ",
    "test": "mkdirp .nyc_output && npm run test:electron && npm run generate-coverage",
    "test:watch": "mkdirp test/build && watchify test/test.js --debug -o test/build/test.js -t [ browserify-istanbul ]",
    "test:browser": "browserify -t browserify-istanbul test/test.js | browser-run -p 2222 | node test/extract-coverage.js | faucet",
    "test:electron": "browserify -t browserify-istanbul test/test.js | browser-run | node test/extract-coverage.js | faucet ",
    "test:chrome": "browserify -t browserify-istanbul test/test.js | browser-run -b chrome | node test/extract-coverage.js | faucet ",
    "generate-coverage": "nyc report --reporter=lcov",
    "check-coverage": "nyc check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "broadcast-channel": "^1.3.0",
    "jquery": "^3.4.1",
    "js-cookie": "2.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babelify": "^8.0.0",
    "browser-run": "^6.0.0",
    "browserify": "^16.2.3",
    "browserify-istanbul": "^3.0.1",
    "codecov": "^3.5.0",
    "commitizen": "^3.1.1",
    "concat-stream": "^2.0.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dbpedia-entity-lookup": "^0.4.2",
    "faucet": "0.0.1",
    "geonames-entity-lookup": "^1.1.7",
    "getty-entity-lookup": "^1.4.5",
    "git-cz": "^1.8.4",
    "husky": "^2.1.0",
    "mkdirp": "0.5.1",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.12",
    "sinon": "^7.3.2",
    "tape": "^4.10.2",
    "tape-catch": "^1.0.6",
    "tape-promise": "^4.0.0",
    "travis-deploy-once": "^5.0.11",
    "viaf-entity-lookup": "^1.2.5",
    "watch": "1.0.2",
    "watchify": "^3.11.1",
    "wikidata-entity-lookup": "^1.2.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "env": {
            "test": {
              "plugins": [
                "istanbul"
              ]
            }
          }
        }
      ]
    ]
  }
}
